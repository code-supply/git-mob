coauthors_file=$1
mob_list_file=$2

mob_list="$(cat "$mob_list_file")"

generated="$(jq \
  --raw-output \
  --join-output \
  --argjson mob "$mob_list" \
  "$(cat <<'EOF'
  .coauthors
| to_entries
| map(" \(.key) '\(.value.name)' \(if [.key] | inside($mob) then "on" else "off" end)")[]
EOF
)" < "$coauthors_file"
)"

echo "0 0 10$generated"
