coauthors_file=$1
template_file=$2
mob_list_file=$3

# shellcheck disable=SC2016
jq \
  -c \
  --null-input \
  --raw-output \
  --args \
  '$ARGS.positional' \
  -- \
  "${@:4}" \
  > "$mob_list_file"

printed="$(git-mob-print "$coauthors_file" "$mob_list_file")"

if [[ -z "$printed" ]]
then
  true > "$template_file"
else
  echo -e "\n\n$printed" > "$template_file"
fi

echo "$printed"
